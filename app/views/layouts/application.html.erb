<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tealeaf Academy - Postit</title>
    <meta name="description" content="">
    <meta name="author" content="">
    <%= stylesheet_link_tag "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>
  </head>
  <body>
    <header> 
      <% if false %>
      <div class='well '>
 Params<%= params.inspect %>
 <br />
   Current user <%= "is #{current_user }" %>
   <br />
   Logged in? <%= logged_in? %>
   <br />
   Session user id <%= session[:user_id] %>
   <br />
   controller action: <%= controller.action_name %>
   <br />
   post id: <%= @post %>
   <br />
   comment id: <%= @comment %>
   <br />
   last Post: <%= Post.last.title %> by<%= Post.last.user_id %>
   <br />
   last comment:  <%= Comment.last.body %> by  <%= Comment.last.user_id %>
   <br />
   Just submitted?<%= comment_just_submitted %>
   <br />
   user votes: <%= user_votes %>
<br />

    last post votes <%= post_votes(Post.last) %>
<br />
<%= already_voted(Post.last) %>

</div>
<% end %>

      <div class="navbar">
        <div class="navbar-inner">
          <div class="container">
            <%= render 'layouts/navigation' %>
          </div>
        </div>
      </div>
    </header>
    <div id="main" role="main">
      <div class="container">
         <div class="row">
          <div class="span12">
            <%= render 'layouts/messages' %>
            <%= yield %>
          </div>
        </div>
        <footer>
        </footer>
      </div> <!--! end of .container -->
    </div> <!--! end of #main -->


  </body>
</html>
