<%= render 'shared/title_format', title: 'All Posts' %>

  <%= render 'display_a_post',  post: Post.last if post_just_submitted %>

  <% @posts.sort_by{ |c| c.total_votes}.reverse.each do |post| %>
    <%= render 'display_a_post',  post: post unless post_just_submitted && post == Post.last %>
  <% end %>






