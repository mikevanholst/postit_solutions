<div class = 'span0 well text-center'>
    <%= link_to  vote_"#{item}"_path("#{item}", vote: true), method: 'post'  do %>
      <i class='icon-arrow-up'></i>
     <% end %>
  <br />
 <%= "#{item}".total_votes %>
  <br />
   <%= link_to vote_"#{item}"_path("#{item}", vote: false), method: 'post'  do %>
      <i class='icon-arrow-down'></i>
     <% end %>
  </div>